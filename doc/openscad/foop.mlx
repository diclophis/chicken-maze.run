 <filter name="Ambient Occlusion">
  <Param enum_cardinality="2" enum_val1="per-Face (deprecated)" value="0" isxmlparam="0" type="RichEnum" description="Occlusion mode:" name="occMode" enum_val0="per-Vertex" tooltip="Occlusion may be calculated per-vertex or per-face, color and quality will be saved in the chosen component."/>
  <Param value="0.3" isxmlparam="0" type="RichFloat" description="Directional Bias [0..1]" name="dirBias" tooltip="The balance between a uniform and a directionally biased set of lighting direction&lt;br>: - 0 means light came only uniformly from any direction&lt;br> - 1 means that all the light cames from the specified cone of directions &lt;br> - other values mix the two set of lighting directions "/>
  <Param value="512" isxmlparam="0" type="RichInt" description="Requested views" name="reqViews" tooltip="Number of different views uniformly placed around the mesh. More views means better accuracy at the cost of increased calculation time"/>
  <Param z="-0.6049" isxmlparam="0" type="RichPoint3f" description="Lighting Direction" y="1.5564" x="1.7005" name="coneDir" tooltip="Number of different views placed around the mesh. More views means better accuracy at the cost of increased calculation time"/>
  <Param value="66" isxmlparam="0" type="RichFloat" description="Cone amplitude" name="coneAngle" tooltip="Number of different views uniformly placed around the mesh. More views means better accuracy at the cost of increased calculation time"/>
  <Param value="true" isxmlparam="0" type="RichBool" description="Use GPU acceleration" name="useGPU" tooltip="Only works for per-vertex AO. In order to use GPU-Mode, your hardware must support FBOs, FP32 Textures and Shaders. Normally increases the performance by a factor of 4x-5x"/>
  <Param value="512" isxmlparam="0" type="RichInt" description="Depth texture size(should be 2^n)" name="depthTexSize" tooltip="Defines the depth texture size used to compute occlusion from each point of view. Higher values means better accuracy usually with low impact on performance"/>
 </filter>

 <filter name="Parametrization: Trivial Per-Triangle">
  <Param description="Quads per line" isxmlparam="0" value="0" name="sidedim" type="RichInt" tooltip="Indicates how many triangles have to be put on each line (every quad contains two triangles)&#xa;Leave 0 for automatic calculation"/>
  <Param description="Texture Dimension (px)" isxmlparam="0" value="1024" name="textdim" type="RichInt" tooltip="Gives an indication on how big the texture is"/>
  <Param description="Inter-Triangle border (px)" isxmlparam="0" value="2" name="border" type="RichInt" tooltip="Specifies how many pixels to be left between triangles in parametrization domain"/>
  <Param enum_val0="Basic" description="Method" enum_val1="Space-optimizing" isxmlparam="0" value="1" name="method" type="RichEnum" tooltip="Choose space optimizing to map smaller faces into smaller triangles in parametrizazion domain" enum_cardinality="2"/>
 </filter>

 <filter name="Transfer: Vertex Color to Texture">
  <Param description="Texture file" isxmlparam="0" value="shape-0_tex.png" name="textName" type="RichString" tooltip="The texture file to be created"/>
  <Param description="Texture width (px)" isxmlparam="0" value="1024" name="textW" type="RichInt" tooltip="The texture width"/>
  <Param description="Texture height (px)" isxmlparam="0" value="1024" name="textH" type="RichInt" tooltip="The texture height"/>
  <Param description="Overwrite texture" isxmlparam="0" value="false" name="overwrite" type="RichBool" tooltip="if current mesh has a texture will be overwritten (with provided texture dimension)"/>
  <Param description="Assign texture" isxmlparam="0" value="false" name="assign" type="RichBool" tooltip="assign the newly created texture"/>
  <Param description="Fill texture" isxmlparam="0" value="true" name="pullpush" type="RichBool" tooltip="if enabled the unmapped texture space is colored using a pull push filling algorithm, if false is set to black"/>
 </filter>
</FilterScript>
