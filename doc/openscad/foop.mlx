  <Param tooltip="Gives an indication on how big the texture is" isxmlparam="0" type="RichInt" description="Texture Dimension (px)" value="1024" name="textdim"/>
  <Param tooltip="Specifies how many pixels to be left between triangles in parametrization domain" isxmlparam="0" type="RichInt" description="Inter-Triangle border (px)" value="2" name="border"/>
